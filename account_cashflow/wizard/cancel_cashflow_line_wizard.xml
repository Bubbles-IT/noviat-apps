<?xml version="1.0" ?>
<openerp>
	<data>

    <!-- Set selected Cash Flow lines to Draft -->

    <record id="view_cancel_cashflow_line" model="ir.ui.view">
      <field name="name">cancel.cashflow.line.form</field>
      <field name="model">cancel.cashflow.line</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form string="Set selected Cash Flow lines to Draft">
          <label string="" colspan="4"/>
          <label colspan="4" string="Are you sure you want to set the selected Cash Flow lines to Draft ?"/>
          <label string="" colspan="4"/>
          <separator colspan="4"/>
          <group>
            <label string="" colspan="2"/>
            <button icon="gtk-cancel" special="cancel" string="Cancel"/>
            <button icon="gtk-execute" string="Set to Draft" name="cancel_lines" type="object"/>
          </group>
        </form>
      </field>
    </record>
    
    <record id="action_cancel_cashflow_line" model="ir.actions.act_window">
      <field name="name">Set selected Cash Flow lines to Draft</field>
      <field name="res_model">cancel.cashflow.line</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
      <field name="view_id" ref="view_cancel_cashflow_line"/>
      <field name="help">Set selected Cash Flow lines to Draft.</field>
      <field name="target">new</field>
    </record>

    <record model="ir.values" id="action_cancel_cashflow_line_values">
      <field name="object" eval="1" />
      <field name="name">Set selected Cash Flow lines to Draft</field>
      <field name="key2">client_action_multi</field>
      <field name="value" eval="'ir.actions.act_window,' +str(ref('action_cancel_cashflow_line'))" />
      <field name="model">account.cashflow.line</field>
    </record>

    <!-- Set selected Cash Flow Provision lines to Draft -->

    <record id="view_cancel_cashflow_provision_line" model="ir.ui.view">
      <field name="name">cancel.cashflow.provision.line.form</field>
      <field name="model">cancel.cashflow.provision.line</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form string="Set selected Cash Flow Provision lines to Draft">
          <label string="" colspan="4"/>
          <label colspan="4" string="Are you sure you want to set the selected Cash Flow Provision lines to Draft ?"/>
          <label string="" colspan="4"/>
          <separator colspan="4"/>
          <group>
            <label string="" colspan="2"/>
            <button icon="gtk-cancel" special="cancel" string="Cancel"/>
            <button icon="gtk-execute" string="Set to Draft" name="cancel_lines" type="object"/>
          </group>
        </form>
      </field>
    </record>

    <record id="action_cancel_cashflow_provision_line" model="ir.actions.act_window">
      <field name="name">Set selected Cash Flow Provision lines to Draft</field>
      <field name="res_model">cancel.cashflow.provision.line</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
      <field name="view_id" ref="view_cancel_cashflow_provision_line"/>
      <field name="help">Set selected Cash Flow Provision lines to Draft.</field>
      <field name="target">new</field>
    </record>

    <record model="ir.values" id="action_cancel_cashflow_provision_line_values">
      <field name="object" eval="1" />
      <field name="name">Set selected Cash Flow Provision lines to Draft</field>
      <field name="key2">client_action_multi</field>
      <field name="value" eval="'ir.actions.act_window,' +str(ref('action_cancel_cashflow_provision_line'))" />
      <field name="model">account.cashflow.provision.line</field>
    </record>
    
  </data> 
</openerp>
