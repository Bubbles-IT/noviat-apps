<?xml version="1.0" ?>
<openerp>
  <data>

    <record id="view_move_from_bank_form" model="ir.ui.view">
      <field name="name">account.bank.statement.voucher.inherit</field>
      <field name="model">account.move</field>
      <field name="inherit_id" ref="account_bank_statement_voucher.view_move_from_bank_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='line_id']//field[@name='analytic_account_id']" position="replace">
          <field name="analytics_id" context="{'journal_id':parent.journal_id}" groups="analytic.group_analytic_accounting"/>
        </xpath>
      </field>
    </record>

  </data>
</openerp>
